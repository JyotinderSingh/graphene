{
  "name": "@jyotinder/graphene",
  "version": "1.0.0",
  "description": "tiny in-memory graph database",
  "main": "dist/graphene.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/JyotinderSingh/graphene"
  },
  "scripts": {
    "run": "node dist/graphene.js",
    "build": "rm -rf dist && tsc",
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "npm run lint && env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' cd tests && mocha --parallel -r ts-node/register ./tests/**/*.ts"
  },
  "author": "Jyotinder Singh",
  "license": "MIT",
  "dependencies": {
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "chai": "^4.3.6",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "mocha": "^9.2.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0"
  }
}
